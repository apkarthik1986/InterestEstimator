{
  "name": "Flutter Development",
  "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
  "features": {
    "ghcr.io/jarrodcolburn/features/flutter-sdk:0": {
      "version": "3.24.5"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "Dart-Code.dart-code",
        "Dart-Code.flutter"
      ]
    }
  },
  "postCreateCommand": "flutter --version && flutter pub get",
  "forwardPorts": [],
  "remoteUser": "vscode"
}
